<!DOCTYPE html>
<html>
<head>
  <title>Golden Gate Wrestling Club</title>
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= content_for :styles -%>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="page-wrap">
      <div class="container">
          <div class="row">
              <div class="span12">
                  <div id="page-frame">
                      <div id="page-header" class="floating-element">
                          <ul id="page-tabs">
                              <li class="active">
                                  <a href="<%= root_url -%>">HOME</a>
                              </li>
                              <li>
                                  <a href="<%= membership_url -%>">MEMBERSHIP</a>
                              </li>
                              <li>
                                  <a href="<%= calendar_url -%>">CALENDAR</a>
                              </li>
<!--
                              <li>
                                  <a href="#">GALLERY</a>
                              </li>
-->
                              <li class="last">
                                  <a href="<%= about_url -%>">ABOUT</a>
                              </li>
                          </ul>
                      </div>

                      <div id="page-body">
                          <%= yield %>
                          <div class="clearfix"></div>
                      </div>

                      <div id="page-footer"></div>

                      <div class="clearfix"></div>
                  </div>
              </div>
          </div>
      </div>
  </div>

  <script src="http:////ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js"></script>
  <%= javascript_include_tag "application" %>
  <%= content_for :scripts -%>
</body>
</html>
